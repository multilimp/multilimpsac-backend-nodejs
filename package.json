{
  "name": "multilimp-backend",
  "description": "Backend ERP MULTILIMP SAC",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn index.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "generate": "prisma generate",
    "start": "node dist/index.js",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "seed": "npx prisma db seed",
    "postinstall": "prisma generate"
  },
  "author": "multilimp",
  "license": "MIT",
  "_moduleAliases": {
    "@": "dist/src",
    "@app": "dist/src/app",
    "@entities": "dist/src/entities",
    "@features": "dist/src/features",
    "@widgets": "dist/src/widgets",
    "@pages": "dist/src/pages",
    "@processes": "dist/src/processes",
    "@shared": "dist/src/shared",
    "@modules": "dist/src/modules"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.799.0",
    "@faker-js/faker": "^10.1.0",
    "@langchain/community": "^0.3.55",
    "@langchain/google-genai": "^0.2.17",
    "@prisma/client": "^6.19.0",
    "@types/glob": "^8.1.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "formidable": "^3.5.4",
    "glob": "^11.0.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.33",
    "module-alias": "^2.2.3",
    "pg": "^8.16.3",
    "winston": "^3.13.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/graphql-fields": "^1.3.9",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "eslint": "8.39.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "nodemon": "3.0.1",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}